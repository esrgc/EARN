@model IEnumerable<string>
@if (Model != null) { 
    <div class="inline-container tag-list">
        @foreach (var i in Model) { 
            <div class="tag">
                <span class="label label-default">
                    @i&nbsp;<a data-value="@i"><i class="icon-remove"></i></a>
                </span>
            </div>
        }
    </div>
    <div class="clear"></div>
}