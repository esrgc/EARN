@model IEnumerable<string>
@if (Model != null) { 
    <div class="inline-container tag-list">
        @if (Model.Count() == 0) { 
            <p></p>
        }
        @foreach (var i in Model) { 
            <div class="tag">
                <span class="label label-default">
                    @i&nbsp;<a data-value="@i"><i class="fa fa-times"></i></a>
                </span>
            </div>
        }
    </div>
    <div class="clear"></div>
}